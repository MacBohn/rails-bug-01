<h1>Pithy quotes</h1>

<% @quotes.each do |quote| %>
  <section class="quote-container">
    <blockquote class="quote">
      <%= quote.text %>
      <footer class="author">
        <%= quote.author %>
      </footer>
    </blockquote>
    <div class="created">
    <% logger.debug("*" * 60) %>
    <% logger.debug(quote.inspect) %>
  <% if quote.created_at !=nil %>
    <%= time_ago_in_words(quote.created_at) %> ago
  <%  else %>
    <%= "Original Date Unknown" %>
<% end %>
    </div>
  </section>
<% end %>
